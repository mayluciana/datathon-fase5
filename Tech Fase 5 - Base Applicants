úsculas\n","    objetivo_profissional = objetivo_profissional.lower()\n","\n","    # 2. Remove o texto entre parênteses\n","    objetivo_profissional = re.sub(r'\\([^)]*\\)', '', objetivo_profissional)\n","\n","    # 3. Continua com as outras limpezas\n","    objetivo_profissional = re.sub(r\"(https?:\\/\\/)?([\\da-z\\.-]+)\\.([a-z\\.]{2,6})([\\/\\w \\.-]*)\", \" \", objetivo_profissional) # remove links\n","    objetivo_profissional = remove_punctuation(objetivo_profissional)\n","    objetivo_profissional = normalize_accents(objetivo_profissional)\n","    objetivo_profissional = re.sub(re.compile(r\" +\"), \" \", objetivo_profissional) # remove espaços duplos\n","\n","    # 4. Remove espaços no início/fim e retorna\n","    return objetivo_profissional.strip()"],"metadata":{"id":"gNhJYbia536d","executionInfo":{"status":"ok","timestamp":1752539169690,"user_tz":180,"elapsed":16,"user":{"displayName":"luciana may","userId":"09308515857938850200"}}},"execution_count":104,"outputs":[]},{"cell_type":"code","source":["def normalize_str(objetivo_profissional):\n","    objetivo_profissional = objetivo_profissional.lower()\n","    objetivo_profissional = re.sub(\"(https?:\\/\\/)?([\\da-z\\.-]+)\\.([a-z\\.]{2,6})([\\/\\w \\.-]*)\", \" \",objetivo_profissional) #remove links\n","    objetivo_profissional = remove_punctuation(objetivo_profissional)\n","    objetivo_profissional = normalize_accents(objetivo_profissional)\n","    objetivo_profissional = re.sub(re.compile(r\" +\"), \" \",objetivo_profissional) #remove espaços duplos\n","    return objetivo_profissional\n","    #return \" \".join([w for w in text.split()])"],"metadata":{"id":"onqLrnT8d7b8","executionInfo":{"status":"ok","timestamp":1752539169693,"user_tz":180,"elapsed":16,"user":{"displayName":"luciana may","userId":"09308515857938850200"}}},"execution_count":105,"outputs":[]},{"cell_type":"code","source":["import unicodedata\n","import re\n","import string"],"metadata":{"id":"0IPi7NZ8eeNj","executionInfo":{"status"